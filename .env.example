# Run ./setup.sh to generate this file automatically

# Cloudflare API Token (scoped to your domain)
CLOUDFLARE_API_TOKEN=your_api_token_here

# Domain for container hostnames
# Example: if set to "example.com", container "web" becomes "web.example.com"
CLOUDFLARE_DOMAIN=example.com

# Zone ID for your domain
CLOUDFLARE_ZONE_ID=your_zone_id_here

# Account ID
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Tunnel Configuration (created by setup.sh)
CLOUDFLARE_TUNNEL_NAME=docker-tunnel
CLOUDFLARE_TUNNEL_ID=your_tunnel_id_here
CLOUDFLARE_TUNNEL_TOKEN=your_tunnel_token_here
